gconftool-2 \
  --get "/apps/gnome-terminal/global/profile_list"

gconftool-2 \
  --set "/apps/gnome-terminal/global/profile_list" \
  --type list \
  --list-type string \
  "[Default,HD-1024-Black,HD-1024-DarkBeige,HD-1024-DarkBlue,HD-1024-DarkPurple,HD-1024-RedWine,HD-1280-Black,HD-1280-DarkBeige,HD-1280-DarkBlue,HD-1280-DarkPurple,HD-1280-RedWine,HD-1600-Black,HD-1600-DarkBeige,HD-1600-DarkBlue,HD-1600-DarkPurple,HD-1600-RedWine,Remote1024,Remote1280,Remote1600]"

gconftool-2 \
  --get "/apps/gnome-terminal/global/profile_list"

