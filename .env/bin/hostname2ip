gRe=re.compile("(https*://)([^/|:]*)(.*)")

lines = sys.stdin.readlines()
import sys
for line in lines:
  mo=gRe.match(line)
  if mo:
    pre=mo.group(1)
    hostname=mo.group(2)
    post=mo.group(3)

    #print "pre = " + pre
    #print "hostname = " + hostname
    #print "post = " + post
    ip=socket.getaddrinfo(hostname, 0)[0][4][0]
    print((line.rstrip() + " -> " + pre + ip + post))
  else:
    print(line)
